{
  "name": "dtpicker",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "webpack-cli": "^3.3.12",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "cross-env": "^7.0.2",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-scrollbar": "^0.5.6",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build",
    "clean": "rimraf lib dist es",
    "lint": "eslint src",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src/lib/Datetimepicker.jsx --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src/lib/Datetimepicker.jsx --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development npx webpack",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production npx webpack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/barmaleeo/dtpicker"
  }
}
